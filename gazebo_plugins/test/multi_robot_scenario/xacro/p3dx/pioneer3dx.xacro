<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="pioneer3dx">
  
  
  <!-- Import Rviz colors -->
  <xacro:include filename="$(find gazebo_plugins)/test/multi_robot_scenario/xacro/materials.xacro" />
  
  <xacro:include filename="$(find gazebo_plugins)/test/multi_robot_scenario/xacro/p3dx/pioneer3dx_body.xacro"/>
  <xacro:pioneer3dx_body />
  
  <xacro:include filename="$(find gazebo_plugins)/test/multi_robot_scenario/xacro/laser/hokuyo.xacro"/>
  <xacro:front_laser xyz="0.16 0 0.1" rpy="0 0 0" />
  
  <xacro:include filename="$(find gazebo_plugins)/test/multi_robot_scenario/xacro/camera/camera.xacro"/>
  <xacro:cam_front xyz="0 0 0.5" rpy="0 0 0" />
  
</robot>
